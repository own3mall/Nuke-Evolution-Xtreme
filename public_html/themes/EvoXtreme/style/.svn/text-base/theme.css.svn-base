/*
 * 1. Reset CSS
 *
 * html5doctor.com Reset Stylesheet (Eric Meyer's Reset Reloaded + HTML5 baseline)
 * v1.6.1 2010-09-17 | Authors: Eric Meyer & Richard Clark
 * html5doctor.com/html-5-reset-stylesheet/
 ----------------------------------------------------------------*/

html, body, div, span, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
abbr, address, cite, code, del, dfn, em, img, ins, kbd, q, samp,
small, strong, sub, sup, var, b, i, dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, figcaption, figure,
footer, header, hgroup, menu, nav, section, summary,
time, mark, audio, video {
	border: 0;
	font-size: 100%;
	margin: 0;
	padding: 0;
}

article, aside, details, figcaption, figure,
footer, header, hgroup, menu, nav, section {
	display: block;
}

blockquote, q {
	quotes: none;
}

blockquote:before, blockquote:after,
q:before, q:after {
	content: "";
	content: none;
}

ins {
	background-color: #ff9;
	color: #000;
	text-decoration: none;
}

mark {
	background-color: #ff9;
	color: #000;
	font-style: italic;
	font-weight: bold;
}

del {
	text-decoration: line-through;
}

abbr[title], dfn[title] {
	border-bottom: 1px dotted;
	cursor: help;
}

hr {
	display: block;
	height: 1px;
	border: 0;
	border-top: 1px solid #ccc;
	margin: 1em 0;
	padding: 0;
}

input, select {
	vertical-align: middle;
}

/*
 * 2. Primary page styles
 ----------------------------------------------------------------*/

body {
	background-color: #5A686F;
	color: #000;
	font-family: Tahoma, Verdana, Arial, sans-serif;
	font-size: 11px;
	margin: 0;
	padding: 0;
}

a {
	color: #256a8e;
	text-decoration: none;
}

a:hover {
	color: #359cd2;
}

/* These selection declarations have to be separate
   No text-shadow: twitter.com/miketaylr/status/12228805301
   Also: hot pink! */
::-moz-selection {
	background: #FF5E99;
	color: #fff;
	text-shadow: none;
}

::selection {
	background: #FF5E99;
	color: #fff;
	text-shadow: none;
}

#container {
	border-left: 1px solid #243E54;
	border-right: 1px solid #243E54;
	box-shadow: -5px 0 5px #3E5866, 5px 0 5px #3E5866;
	margin: 0 auto;
	min-width: 990px;
	max-width: 1600px;
}

/*
 * 3. Page elements
 ----------------------------------------------------------------*/

textarea, select, input {
	background-color: #fff;
	border: 1px solid #ccc;
	border-radius: 3px;
	font-size: 11px;
	padding: 2px;
}

p {
	padding-bottom: 5px;
	text-decoration: none;
}

p, table {
	text-decoration: none;
}

hr {
	background-color: #cccccc;
	border: 0;
	height: 1px;
	margin: 5px 0;
	width: 100%;
}

ul {
	list-style-type: disc;
	padding-bottom: 5px;
}

h1, h2, h3, h4, h5, h6 {
	color: #999;
	font-weight: normal;
}

h1 {
	font-size: 30px;
	line-height: 1;
	margin-bottom: 5px;
}

h2 {
	font-size: 20px;
	margin-bottom: 7.5px;
}

h3 {
	font-size: 15px;
	line-height: 1;
	margin-bottom: 10px;
}

h4 {
	font-size: 12px;
	line-height: 1.25;
	margin-bottom: 12.5px;
}

h5 {
	font-size: 10px;
	font-weight: bold;
	margin-bottom: 15px;
}

h6 {
	font-size: 10px;
	font-weight: bold;
}

/*
 * 4. Page header
 ----------------------------------------------------------------*/

header {
	background: url('../images/hd_stretch.png') repeat-x;
	height: 267px;
}

header > hgroup {
	height: 143px;
}

header h1 {
	background-image: url('../images/logo.png');
	float: left;
	height: 143px;
	margin: 0;
	padding: 0;
	text-indent: -9999px;
	width: 399px;
}

header h2 {
	left: -9999px;
	position: absolute;
}

header nav {
	border-radius: 0 0 5px 5px;
	float: right;
	margin-right: 10px;
}

header nav li {
	cursor: pointer;
	float: left;
	list-style-type: none;
}

header nav li:first-child a {
	border-radius: 0 0 0 5px;
}

header nav li:last-child a {
	border-radius: 0 0 5px 0;
}

header nav li:hover a,
header nav li.current a {
	border-radius: 0 0 5px 5px;
	box-shadow: 0 0 3px rgba(0, 0, 0, 0.6);
	position: relative;
	z-index: 1000;
}

header nav li a {
	background-color: #396a83;
	color: #fff;
	display: block;
	float: left;
	font-size: 12px;
	height: 24px;
	line-height: 24px;
	padding: 0 20px;
	text-align: center;
	text-decoration: none;
	-moz-transition: all 0.2s ease-in-out 0s;
	-o-transition: all 0.2s ease-in-out 0s;
	-webkit-transition: all 0.2s ease-in-out 0s;
}

header nav li a:hover,
header nav li.current a {
	background-color: #102B3A;
	color: #81D3FF;
	height: 34px;
	line-height: 34px;
	text-shadow: 0 -1px 0 #04080A;
}

#header-ads {
	clear: right;
	float: right;
	height: 60px;
	margin: 35px 25px 0 0;
	width: 468px;
}

/*
 * 5. User interaction
 ----------------------------------------------------------------*/

#user-interact {
	background: url('../images/post_header.png') repeat-x;
	height: 124px;
}

#welcome-wrap {
	float: left;
	margin: 19px 0 0 9px;
	position: relative;
	width: 252px;
}

#welcome-wrap h3 {
	background-image: url('../images/welcome-top.gif');
	color: #000;
	font-size: 11px;
	font-weight: bold;
	height: 19px;
	margin: 0;
	padding: 11px 12px 0 12px;
	text-shadow: 0 -1px 0 #DCF9B5;
	text-transform: uppercase;
}

#welcome-wrap p {
	background-image: url('../images/welcome-bottom.gif');
	height: 57px;
	padding: 1px 15px 0;
	width: auto;
}

#message-wrap {
	float: right;
	width: 286px;
	margin-top: 18px;
}

#message-wrap h3 {
	background-image: url('../images/message_title.gif');
	color: #4489ae;
	font-size: 18px;
	font-weight: normal;
	height: 28px;
	line-height: 26px;
	margin: 0;
	text-align: center;
	text-decoration: none;
	text-shadow: 0 0 1px #15435b;
	width: inherit;
}

#message-wrap p {
	height: 50px;
	padding: 5px 0 0;
	text-align: center;
	width: inherit;
}

/*
 * 6. Body content wrappers
 ----------------------------------------------------------------*/

#body-wrap {
	background-color: #f5f5f5;
	padding: 5px 10px 0;
	width: auto;
}

#blocks-left-wrap,
#blocks-right-wrap {
	width: 195px;
}

#blocks-left-wrap {
	float: left;
}

#blocks-right-wrap {
	float: right;
}

#center-wrap {
	margin: 0 200px;
}

#center-wrap-left {
	margin-left: 200px;
}

#center-wrap-right {
	margin-right: 200px;
}

#center-wrap-full {
	width: inherit;
}

.sides {
	background-repeat: no-repeat;
	display: block;
	position: absolute;
	width: 100%;
}

.sides span {
	float: right;
}

/*
 * 7. Side body blocks
 ----------------------------------------------------------------*/

.blocks-wrap {
	margin-top: 10px;
	width: 195px;
}

.blocks-wrap:first-child {
	margin-top: 0;
}

.blocks-wrap h4 {
	background-image: url('../images/blocks_01.gif');
	color: #000;
	font-size: 11px;
	font-weight: bold;
	height: 47px;
	line-height: 49px;
	margin: 0;
	text-align: center;
	width: inherit;
}

.blocks-body {
	background: url('../images/blocks_02.gif') repeat-y;
	overflow: hidden;
	padding: 0 15px 5px;
	text-align: left;
	width: 165px;
}

.blocks-footer {
	background-image: url('../images/blocks_03.gif');
	display: block;
	height: 16px;
	width: inherit;
}

.blocks-footer {
	background-image: url('../images/blocks_03.gif');
	display: block;
	height: 16px;
	width: inherit;
}

/*
 * 8. Center content wrapper
 ----------------------------------------------------------------*/

.tables-wrap {
	margin-top: 10px;
}

.tables-wrap:first-child {
	margin-top: 0;
}

.tables-hd {
	background: url('../images/tables_02.gif') repeat-x;
	height: 22px;
	position: relative;
	width: auto;
}

.tables-hd span {
	background-image: url('../images/tables_01.gif');
	height: 22px;
}

.tables-hd span > span {
	background-image: url('../images/tables_03.gif');
	height: 22px;
	width: 38px;
}

.tables-body {
	background-color: #f8f7f7;
	position: relative;
}

.tables-body > span {
	background: url('../images/tables_04.gif') repeat-y;
	height: 100%;
	left: 0;
	position: absolute;
}

.tables-body > span > span {
	background: url('../images/tables_06.gif') repeat-y;
	height: 100%;
	position: absolute;
	right: 0;
	width: 17px;
}

.tables-body-content {
	margin: 0 17px;
	text-align: left;
	overflow: hidden;
	position: relative;
	z-index: 1;
}

.tables-ft {
	background: url('../images/tables_08.gif') repeat-x;
	height: 22px;
	position: relative;
	width: auto;
}

.tables-ft span {
	background-image: url('../images/tables_07.gif');
	height: 22px;
}

.tables-ft span > span {
	background-image: url('../images/tables_09.gif');
	height: 22px;
	width: 38px;
}

/*
 * 9. Story content wrapper
 ----------------------------------------------------------------*/

.news-story {
	margin-top: 10px;
}

.news-story:first-child {
	margin-top: 0;
}

.story-hd {
	background: url('../images/news_02.gif') repeat-x;
	height: 47px;
	position: relative;
	width: auto;
}

.story-hd span {
	background-image: url('../images/news_01.gif');
	height: 47px;
}

.story-hd span > span {
	background-image: url('../images/news_03.gif');
	height: 47px;
	width: 44px;
}

.story-hd h4 {
	font-size: 11px;
	font-weight: bold;
	line-height: 50px;
	margin: 0;
	padding: 0 60px;
	text-shadow: 0 -1px 0 #fff;
}

.story-bd {
	background-color: #f8f7f7;
	position: relative;
	width: auto;
}

.story-bd .sides {
	background: url('../images/news_04.gif') repeat-y;
	height: 100%;
	left: 0;
	position: absolute;
}

.story-bd .sides span {
	background: url('../images/news_06.gif') repeat-y;
	height: 100%;
	position: absolute;
	right: 0;
	width: 16px;
}

.story-bd article {
	margin: 0 16px;
	overflow: hidden;
	position: relative;
	text-align: left;
	z-index: 1;
}

.story-ft {
	background: url('../images/news_08.gif') repeat-x;
	height: 29px;
	position: relative;
	width: auto;
}

.story-ft span {
	background-image: url('../images/news_07.gif');
	height: 29px;
}

.story-ft span > span {
	background-image: url('../images/news_09.gif');
	height: 29px;
	width: 44px;
}

/*
 * 10. Page footer
 ----------------------------------------------------------------*/

footer {
	background: url('../images/footer-top-bg.gif') repeat-x;
	padding-top: 15px;
}

footer section {
	background: url('../images/footer-main-bg.gif') repeat-x;
	color: #fff;
	min-height: 100px;
	max-height: 300px;
	padding: 2px 10px 0 10px;
	text-align: center;
	width: auto;
}

footer .copyright {
	color: #fff;
}

footer a {
	color: #359cd2;
}

footer a:hover {
	text-decoration: underline;
}

footer p {
	background: url('../images/footer-bottom-bg.gif') repeat-x;
	height: 21px;
	overflow: hidden;
	padding: 0;
}

footer p > a {
	background-image: url('../images/copyright.png');
	display: block;
	height: 5px;
	margin: 11px 0 0 11px;
	text-indent: -9999px;
	width: 368px;
}

/*
 * 11. Clearfix
 *
 * The Magnificent Clearfix: Updated to prevent margin-collapsing on child elements.
 * j.mp/bestclearfix
 ----------------------------------------------------------------*/

.clearfix:before,
.clearfix:after {
	content: "\0020";
	display: block;
	height: 0;
	overflow: hidden;
}

.clearfix:after {
	clear: both;
}

.clearfix {
	zoom: 1;
}

input {
	color: #000;
	font-size: 11px;
	background-color: #fff;
	border: #cccccc 1px solid;
}

input.noborder {
	color: #fff;
	font: 11px arial, helvetica, sans-serif;
	border: 0 solid #fff;
}

input.sub {
	color: #000;
	font-size: 11px;
	background-color: #fff;
	border: #cccccc 1px solid;
}

/* The buttons used for bbCode styling in message post */
input.button {
	color: #000;
	font-size: 11px;
	background-color: #fff;
	border: #cccccc 1px solid;
}

/* The main submit, go and preview buttons */
input.mainoption {
	color: #000;
	font-size: 11px;
	font-weight: normal;
	border: 1px solid #fff;
	border-right-color: #A9B8C2;
	border-bottom-color: #A9B8C2;
	background: #f0f0f0;
}

/* None-bold submit, go and preview buttons */
input.liteoption {
	color: #000;
	font-size: 11px;
	font-weight: normal;
	border: 1px solid #fff;
	border-right-color: #A9B8C2;
	border-bottom-color: #A9B8C2;
	background: #eeeeee;

}