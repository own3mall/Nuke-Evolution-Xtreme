/* ChromoWB Theme Style.css */

.bottom {
	z-index: 2;
	left: 0;
	width: 100%;
	position: absolute;
	top: 594px;
	height: 30px;
	text-align: center;
}

a:link {
	font-size: 11px;
	color: #cb0000;
	font-family: Helvetica, Verdana, Geneva, Arial, sans-serif;
	text-decoration: none;
}

a:visited {
	font-size: 11px;
	color: #cb0000;
	font-family: Helvetica, Verdana, Geneva, Arial, sans-serif;
	text-decoration: none;
}

a:active {
	font-size: 11px;
	color: #cb0000;
	font-family: Helvetica, Verdana, Geneva, Arial, sans-serif;
	text-decoration: none;
}

a:hover {
	font-size: 11px;
	color: #b95b5b;
	font-family: Helvetica, Verdana, Geneva, Arial, sans-serif;
	text-decoration: none;
}

body {
	margin: 0;
	font-size: 12px;
	cursor: default;
	color: #b95b5b;
	background-color: #000000;
	font-family: Helvetica, Verdana, Geneva, Arial, sans-serif;
	text-decoration: none;
}

p {
	font-size: 12px;
	background: inherit;
	color: #cb0000;
	line-height: 1.4;
	font-family: Helvetica, Verdana, Geneva, Arial, sans-serif;
	text-decoration: none;
}

/* Changes colors for: Main Menu Block, Table Box, Story Box */
table {
	font-size: 11px;
	color: #ededed;
	font-family: Helvetica, Verdana, Geneva, Arial, sans-serif;
	text-decoration: none;
}

/* Changes color for: Story Box - Post By Line */
td.content {
	font-size: 12px;
	color: #ededed;
	text-decoration: none;
	font-family: Helvetica, Verdana, Geneva, Arial, sans-serif;
}

td.extra {
	border-right: 1px solid #505050;
}

td.extras {
	background: #F0F0F0;
	color: inherit;
	border: 1px solid;
	border-color: #ffffff #707070 #707070 #ffffff;
}

textarea {
	background: url(../../../themes/ChromoWB/images/input.gif) #2d2f31 repeat-x;
	border: 1px solid;
	border-color: #2a2a2a #8a8a8a #8a8a8a #2a2a2a;
	font-size: 11px;
	color: #b95b5b;
}

p {
	font-size: 11px;
	color: #c3c3c3;
	line-height: 1.4;
	font-family: Helvetica, Verdana, Geneva, Arial, sans-serif;
	text-decoration: none;
}

hr {
	border-top: 1px solid #303030;
	border-bottom: 1px solid #787878;
	height: 2px;
	width: 100%;
}

UL {
	list-style-type: square;
}

.center {
	text-align: center;
}

.justify {
	text-align: justify;
}

.right {
	text-align: right;
}

h1 {
	font-weight: bold;
	font-size: 11px;
	color: #fcfcfc;
	font-family: arial, helvetica, verdana, sans-serif;
	letter-spacing: 0;
	text-decoration: none;
}

.heading {
	font-weight: bold;
	font-size: 11px;
	color: #ffffff;
	font-family: Helvetica, Verdana, Geneva, Arial, Tahoma, sans-serif;
	letter-spacing: 0;
	text-decoration: none;
}

.heading1 {
	font-weight: bold;
	font-size: 11px;
	color: #ffffff;
	font-family: Helvetica, Verdana, Geneva, Arial, Tahoma, sans-serif;
	letter-spacing: 0;
	text-decoration: none;
}

.blocktitle {
	font-weight: bold;
	font-size: 11px;
	color: #b95b5b;
	font-family: Helvetica, Verdana, Geneva, Arial, sans-serif;
	letter-spacing: 0;
	text-decoration: none;
}

.storytitle {
	font-weight: bold;
	font-size: 11px;
	color: #b95b5b;
	font-family: Helvetica, Verdana, Geneva, Arial, sans-serif;
	letter-spacing: 0;
	text-decoration: none;
}

td.story {
	background-color: #F0F0F0;
	color: #cb0000;
	font-size: 11px;
	font-family: Helvetica, Verdana, Geneva, Arial, sans-serif;
	border: 1px solid;
	border-color: #ffffff #707070 #707070 #ffffff;
}

select {
	color: #ffffff;
	font-size: 11px;
	font-family: Helvetica, Verdana, Geneva, Arial, sans-serif;
	background-color: #1c1d1f;
	border: 1px solid;
	border-color: #2a2a2a #8a8a8a #8a8a8a #2a2a2a;
}

